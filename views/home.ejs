<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">
    <link rel="stylesheet" href="Homepage.css">
    <title>Secret Coffee</title>
  </head>
  <body>
    <%- include('./header.ejs');%>


      <div id="content-container">

        <!-- Carousell-->
      <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner text-center">
          <div class="carousel-item active">
            <img class="d-block w-100" src="Gallery/wallpaper1.jpg" alt="First slide" height="500px">
            <div class="carousel-caption d-flex h-100 align-items-center justify-content-center" style="flex-direction: column;">
              <h1>MORE THAN JUST COFFEE</h1>
              <p >Our beans come from different countries around the world</p>
            </div>
          </div>
          <div class="carousel-item">
            <img  class="d-block w-100" src="Gallery/wallpaper2.jpg" alt="Second slide" height="500px">
            <div class="carousel-caption d-flex h-100 align-items-center justify-content-center" style="flex-direction: column;">
              <h1>Mofsdffda</h1>
              <p>vsdsvsssv</p>
            </div>
          </div>
          <div class="carousel-item">
            <img  class="d-block w-100" src="Gallery/wallpaper3.jpg" alt="Third slide" height="500px">
            <div class="carousel-caption d-flex h-100 align-items-center justify-content-center" style="flex-direction: column;"> 
              <h1>...</h1>
              <p>...</p>
            </div>
          </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>

       <!-- End OfCarousell-->

       
      <div class="beansCountries"> <!--beans countries buttons-->
        <h5 class="header">Our beans are grown around the world</h5>
        <div class="row">
          <div class="col">
            <button type="button" class="btn countries">Colombia</button>
          </div>
          <div class="col">
            <button type="button" class="btn countries">Brazil</button>
          </div>
          <div class="col">
            <button type="button" class="btn countries">Guatemala</button>
          </div>
          <div class="col">
            <button type="button" class="btn countries">Indonesia</button>
          </div>
        </div>
        <div class="row">
          <div class="col">
            <button type="button" class="btn countries">India</button>
          </div>
          <div class="col">
            <button type="button" class="btn countries">Costa rica</button>
          </div>
          <div class="col">
            <button type="button" class="btn countries">Mexico</button>
          </div>
          <div class="col">
            <button type="button" class="btn countries">Ethiopia</button>
          </div>
        </div>
      </div>

      </div>


