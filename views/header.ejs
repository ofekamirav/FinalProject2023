  </head>
  <body>
    <header class="header">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <img src="Gallery/logo.png" width="50" height="50" alt="">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo01" aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarTogglerDemo01">
                <a class="navbar-brand" href="/" style="hover{cursor: pointer;}">Secret Coffee</a>
                <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                    <li class="nav-item">
                        <a class="nav-link" href="#">About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/shop" >Store</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Contact us</a>
                    </li>
  
                      <%if(username!=null){%>
                    <li  class="userwelcome">
                      Hello <%=username%>
                     </li>
                     <%}%>
                </ul>
                <%if(username==null){%>
                <ul class="navbar-nav mr-right">
                    <li class="nav-item">
                        <a class="nav-link"  href="/login" id="login">
                            login
                        </a>
                    </li>
                   
                    <li class="nav-item">
                        <a class="nav-link" href="/register"  id="Register">
                            Register
                        </a>
                    </li>
                    <li>
                        <button type="button" class="btn btn-link"><span class="bi bi-cart black-color"></span></button>
                    </li>
                </ul>
                <%}else{%>
                  <ul class="navbar-nav mr-right">
                    <li class="nav-item">
                        <a class="nav-link"  href="/login" id="login">
                            Manager Page
                        </a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link"  href="/logout" id="logout">
                          Logout
                      </a>
                  </li>
                  <li>
                      <button type="button" class="btn btn-link"><span class="bi bi-cart black-color"></span></button>
                  </li>
                    <%}%>
  
                </ul>
                
            </div>
        </nav>
    </header>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.7.0.min.js" integrity="sha256-2Pmvv0kuTBOenSvLm6bvfBSSHrUJ+3A7x6P5Ebd07/g=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
